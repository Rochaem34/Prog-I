programa {
	funcao inicio() {
		real n1, n2, n3, n4, m1, m2, m3, m4, media
		
		escreva ("Digite número 1: ")
		leia (n1)
		m1 = n1 * 0.1
		
		escreva ("Digite número 2: ")
		leia (n2)
		m2 = n2 * 0.2
		
		escreva ("Digite número 3: ")
		leia (n3)
		m3 = n3 * 0.3
		
		escreva ("Digite número 4: ")
		leia (n4)
		m4 = n4 * 0.4
		
		media = (m1 + m2 + m3 + m4)/4
		
		escreva  ("sua média é: " + media)
	}
}
